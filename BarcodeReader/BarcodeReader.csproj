<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>

    <Version>2.0.1</Version>
    <PackageReleaseNotes>Migrated from .NET Framework to .NET Core</PackageReleaseNotes>

    <RepositoryUrl>https://github.com/speyck/BarcodeReader</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    
    <RootNamespace>speyck.BarcodeReader</RootNamespace>
    <PackageId>speyck.BarcodeReader</PackageId>
    <Authors>speyck</Authors>
    <Company>speyck</Company>
    <Product>speyck.BarcodeReader</Product>
    <Description>BarcodeReader is a facade of the ZXing.Net library and allows you to decode images containing Barcodes.</Description>
    <NeutralLanguage>en-CH</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="5.0.1" />
    <PackageReference Include="ZXing.Net" Version="0.16.6" />
  </ItemGroup>

</Project>
